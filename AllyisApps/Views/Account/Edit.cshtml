@model AllyisApps.ViewModels.Auth.EditOrganizationViewModel

@{
    Layout = "~/Views/Shared/_LayoutJustAForm.cshtml";
    ViewBag.Title = Strings.EditOrganization;
}
@{ ViewData.Add(ControllerConstants.Controller, ControllerConstants.Account); }
@{ ViewData.Add(ActionConstants.Action, ActionConstants.Edit); }

@{ Html.RenderPartial("_OrganizationEditDetailsPartial", Model, ViewData); }

@section scripts
{
    <script type="text/javascript">
        var preserveState = '@(Model != null ? (States.ResourceManager.GetString(Model.Clean(Model.State)) ?? Model.State) : "")';
		var dropdownempty = "@Strings.DropDownEmpty";
		var isSubdomainNameUniqueAction = "@(Url.Action(ActionConstants.IsSubdomainNameUnique))?subdomainName=";
		var oldSubdomainName = '@Model.SubdomainName';
    </script>
    @Scripts.Render("~/bundles/UpdateStateDDL");
	@Scripts.Render("~/bundles/AccountCreateOrg");
}
