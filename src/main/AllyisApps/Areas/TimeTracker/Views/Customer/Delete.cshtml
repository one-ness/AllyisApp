@model AllyisApps.ViewModels.EditCustomerInfoViewModel

@{
    DisplayHints.BreadcrumbNavPartialLocation = "~/Areas/TimeTracker/Views/Shared/_BreadcrumbNavPartial.cshtml";
    DisplayHints.LayoutThemeBundle = "~/Content/Timetracker";
    ViewBag.Title = AllyisApps.Resources.TimeTracker.Views.Customer.Strings.Delete;
}

<h2>@AllyisApps.Resources.TimeTracker.Views.Customer.Strings.Delete</h2>

<h3>@AllyisApps.Resources.TimeTracker.Views.Customer.Strings.DeleteHeader</h3>
<div>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value=@AllyisApps.Resources.TimeTracker.Views.Customer.Strings.Delete class="btn btn-default" /> |
            @Html.ActionLink(AllyisApps.Resources.TimeTracker.Views.Customer.Strings.BackToList, "Index")
        </div>
    }
</div>