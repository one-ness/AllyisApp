@using AllyisApps.Services.BusinessObjects;
@model OrganizationInfo
@{
    ViewBag.Title = AllyisApps.Resources.Views.Account.Strings.OrganizationDetails;
}

<table id="org-details" class="table table-striped table-condensed">
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.Name</label></td>
        <td>@Html.DisplayFor(m => m.Name)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.Subdomain</label></td>
        <td>@Html.DisplayFor(m => m.Subdomain)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.SiteURL</label></td>
        <td>@Html.DisplayFor(m => m.SiteUrl)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.Address</label></td>
        <td>@Html.DisplayFor(m => m.Address)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.Country</label></td>
        <td>@Html.DisplayFor(m => m.Country)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.State</label></td>
        <td>@Html.DisplayFor(m => m.State)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.City</label></td>
        <td>@Html.DisplayFor(m => m.City)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.PostalCode</label></td>
        <td>@Html.DisplayFor(m => m.PostalCode)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.PhoneNumber</label></td>
        <td>@Html.DisplayFor(m => m.PhoneNumber)</td>
    </tr>
    <tr>
        <td><label>@AllyisApps.Resources.Views.Account.Strings.FaxNumber</label></td>
        <td>@Html.DisplayFor(m => m.FaxNumber)</td>
    </tr>
</table>